{
    "name": "railt/reflection",
    "type": "library",
    "description": "The Railt Reflection component provides Reflection API to read GraphQL IDL source code",
    "license": "MIT",
    "keywords": [ "graphql", "idl", "otd", "ast", "reflection", "reader", "introspection" ],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0",
        "railt/parser": "self.version",
        "railt/support": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Reflection\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "ramsey/uuid": "Software UUID generator (~3.0)",
        "ext-com-dotnet": "Windows UUID/GUID generator",
        "psr/cache-implementation": "PSR-6 Cache Implementation",
        "psr/simple-cache-implementation": "PSR-16 Cache Implementation"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
