{
    "name": "railt/railt",
    "type": "library",
    "description": "The Railt Framework.",
    "license": "MIT",
    "keywords": [ "graphql", "idl", "otd", "ast", "framework", "railt", "application" ],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "psr/log": "~1.0",
        "psr/container": "~1.0",
        "hoa/compiler": "~3.17",
        "illuminate/support": "~5.1"
    },
    "replace": {
        "railt/http": "self.version",
        "railt/parser": "self.version",
        "railt/routing": "self.version",
        "railt/container": "self.version",
        "railt/reflection": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/Railt/"
        }
    },
    "require-dev": {
        "railt/webonyx-adapter": "self.version",
        "phpunit/phpunit": "~6.1",
        "monolog/monolog": "~1.9",
        "symfony/finder": "~3.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\": "tests"
        }
    },
    "suggest": {
        "railt/webonyx-adapter": "Webonyx GraphQL Adapter for Railt Framework (~self.version)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
