{
    "name": "serafim/railgun",
    "type": "library",
    "description": "Fast GraphQL API implementation",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.1",
        "yay/yay": "~0.2"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\Railgun\\": "src/"
        }
    },
    "require-dev": {
        "webonyx/graphql-php": "~0.9.0",
        "youshido/graphql": "~1.4",
        "symfony/http-foundation": "~2.8|3.*",
        "illuminate/http": "~5.1",
        "phpunit/phpunit": "~6.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\Railgun\\Tests\\": "tests",
            "Serafim\\Railgun\\Example\\": "examples"
        }
    },
    "suggest": {
        "youshido/graphql": "(~1.4) Youshido GraphQL root driver support",
        "webonyx/graphql-php": "(~0.9) Webonyx GraphQL root driver support",
        "symfony/http-foundation": "(~2.8|3.*) Symfony framework support",
        "illuminate/http": "(~5.1) Laravel (illuminate) framework support"
    },
    "scripts": {
        "docs-make": "daux --destination=../railgun-docs",
        "docs": "daux serve"
    }
}
