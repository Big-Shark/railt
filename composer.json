{
    "name": "railt/railt",
    "type": "library",
    "description": "The Railt Framework.",
    "license": "MIT",
    "keywords": [ "graphql", "idl", "otd", "ast", "framework", "railt", "application" ],
    "homepage": "http://railt.org",
    "version": "dev-master",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "psr/log": "~1.0",
        "psr/container": "~1.0",
        "railt/http": "self.version",
        "railt/parser": "self.version",
        "railt/routing": "self.version",
        "railt/reflection": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/Railt/"
        }
    },
    "require-dev": {
        "railt/webonyx-adapter": "self.version",
        "sami/sami": "~4.0",
        "monolog/monolog": "~1.9",
        "phpunit/phpunit": "~6.1",
        "symfony/finder": "~2.8|3.*|4.*",
        "illuminate/http": "~5.1",
        "symfony/http-foundation": "~2.8|3.*|4.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\": "tests"
        }
    },
    "suggest": {
        "webonyx/graphql-php": "Webonyx GraphQL Adapter for Railt Framework (self.version)."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-master"
        }
    },
    "scripts": {
        "init-devel-mode": [
            "php bin/devel-mode"
        ],
        "api": [
            "php vendor/sami/sami/sami.php update build/docs.config"
        ],
        "post-create-project-cmd": [
            "@init-devel-mode"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
